<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/wizard/db" VERSION="20150119" COMMENT="XMLDB file for Moodle blocks/wizard"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="wizard_name" COMMENT="Default comment for wizard name, please edit me">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="name" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="firstname" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="lastname" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="email" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="address" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="city" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="state" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
       <FIELD NAME="description" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="userfile" TYPE="text" NOTNULL="true" SEQUENCE="false"/> 
         <FIELD NAME="status" TYPE="int" LENGTH="2" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/> 
        <FIELD NAME="timemodified" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>